{
  "name": "dicord-soundbot",
  "version": "1.0.0-dev",
  "description": "A Discord Bot to play sounds",
  "repository": {
    "type": "git",
    "url": "https://github.com/markokajzer/discord-soundbot"
  },
  "keywords": [
    "discord",
    "bot",
    "soundboard",
    "sounds",
    "mp3",
    "nodejs",
    "typescript"
  ],
  "author": "Marko Kajzer",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^11.3.2",
    "lowdb": "^1.0.0",
    "node-opus": "^0.2.6"
  },
  "optionalDependencies": {
    "erlpack": "discordapp/erlpack",
    "uws": "^9.14.0",
    "zlib-sync": "^0.1.4"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.6",
    "@types/lowdb": "^1.0.0",
    "@types/node": "^8.9.5",
    "jasmine": "^3.1.0",
    "nyc": "^11.4.1",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc -p tsconfig.json",
    "lint": "tslint -p tsconfig.json",
    "test": "TS_NODE_TYPE_CHECK=1 nyc jasmine --config=./test/support/jasmine.json",
    "serve": "node dist/bot.js",
    "start": "npm run build && npm run serve",
    "release": "npm run build && npm run lint && npm run test"
  }
}
